#include <stdio.h>

int main() {
    int T;
    scanf("%d", &T);

    while (T--) {
        int n;
        scanf("%d", &n);

        int arr[n];
        long long total_sum = 0, left_sum = 0;

        for (int i = 0; i < n; i++) {
            scanf("%d", &arr[i]);
            total_sum += arr[i];
        }

        int found = 0;
        for (int i = 0; i < n; i++) {
            
            long long right_sum = total_sum - left_sum - arr[i];
            if (left_sum == right_sum) {
                found = 1;
                break;
            }
            left_sum += arr[i];
        }

        if (found)
            printf("YES\n");
        else
            printf("NO\n");
    }

    return 0;
}
