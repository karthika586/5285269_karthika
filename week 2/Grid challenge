#include <stdio.h>
#include <string.h>
#include <stdlib.h>


int cmpChar(const void *a, const void *b) {
    return (*(char*)a - *(char*)b);
}

int main() {
    int t;
    scanf("%d", &t); 

    while (t--) {
        int n;
        scanf("%d", &n);
        char grid[n][101]; 

        for (int i = 0; i < n; i++) {
            scanf("%s", grid[i]);
            
            qsort(grid[i], strlen(grid[i]), sizeof(char), cmpChar);
        }

        int possible = 1; 

        
        for (int col = 0; col < n && possible; col++) {
            for (int row = 0; row < n - 1; row++) {
                if (grid[row][col] > grid[row + 1][col]) {
                    possible = 0; 
                    break;
                }
            }
        }

        if (possible)
            printf("YES\n");
        else
            printf("NO\n");
    }

    return 0;
}
