#include <stdio.h>
#include <stdlib.h>

int main() {
    int n, q;
    scanf("%d %d", &n, &q);

    
    int *seq[n];           
    int seqSizes[n];       
    for (int i = 0; i < n; i++) {
        seq[i] = NULL;
        seqSizes[i] = 0;
    }

    int lastAnswer = 0;

    for (int i = 0; i < q; i++) {
        int type, x, y;
        scanf("%d %d %d", &type, &x, &y);

        int idx = (x ^ lastAnswer) % n; 

        if (type == 1) {
            
            seqSizes[idx]++;
            seq[idx] = realloc(seq[idx], seqSizes[idx] * sizeof(int));
            seq[idx][seqSizes[idx] - 1] = y;
        } 
        else if (type == 2) {
            int valueIndex = y % seqSizes[idx];
            lastAnswer = seq[idx][valueIndex];
            printf("%d\n", lastAnswer);
        }
    }

    
    for (int i = 0; i < n; i++) {
        free(seq[i]);
    }

    return 0;
}
