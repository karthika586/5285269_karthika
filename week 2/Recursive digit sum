#include <stdio.h>
#include <string.h>


long long digitSum(char n[]) {
    long long sum = 0;
    for (int i = 0; n[i] != '\0'; i++) {
        sum += n[i] - '0'; 
    }
    return sum;
}


long long superDigit(long long num) {
    if (num < 10)  
        return num;

    long long sum = 0;
    while (num > 0) {
        sum += num % 10;
        num /= 10;
    }
    return superDigit(sum);
}

int main() {
    char n[100001]; 
    int k;
    scanf("%s %d", n, &k);

    long long sum = digitSum(n);
    sum *= k; 

    printf("%lld\n", superDigit(sum));
    return 0;
}
