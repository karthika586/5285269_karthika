#include <stdio.h>
#include <string.h>
#define MAX 1000   
char* isBalanced(char s[]) {
    char stack[MAX];
    int top = -1;  
    for (int i = 0; i < strlen(s); i++) {
        char c = s[i];
        if (c == '(' || c == '[' || c == '{') {
            stack[++top] = c;
        }
        else if (c == ')' || c == ']' || c == '}') {
            if (top == -1) return "NO";  
            char topChar = stack[top--]; 
            if ((c == ')' && topChar != '(') ||
                (c == ']' && topChar != '[') ||
                (c == '}' && topChar != '{')) {
                return "NO";  
            }
        }
    }
    if (top == -1)
        return "YES";
    else
        return "NO";
}
int main() {
    int n;
    scanf("%d", &n);   
    for (int i = 0; i < n; i++) {
        char s[MAX];
        scanf("%s", s);  
        printf("%s\n", isBalanced(s));
    }
    return 0;
}
