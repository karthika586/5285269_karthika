#include <stdio.h>
int sum(int arr[], int n) {
    int s = 0;
    for (int i = 0; i < n; i++)
        s += arr[i];
    return s;
}

int equalStacks(int h1[], int n1, int h2[], int n2, int h3[], int n3) {
    int top1 = 0, top2 = 0, top3 = 0;
    int sum1 = sum(h1, n1);
    int sum2 = sum(h2, n2);
    int sum3 = sum(h3, n3);

    while (1) {
        // If any stack becomes empty
        if (top1 == n1 || top2 == n2 || top3 == n3)
            return 0;

        // If all sums are equal
        if (sum1 == sum2 && sum2 == sum3)
            return sum1;

        // Remove from the tallest stack
        if (sum1 >= sum2 && sum1 >= sum3)
            sum1 -= h1[top1++];
        else if (sum2 >= sum1 && sum2 >= sum3)
            sum2 -= h2[top2++];
        else
            sum3 -= h3[top3++];
    }
}

int main() {
    int n1, n2, n3;
    scanf("%d %d %d", &n1, &n2, &n3);

    int h1[n1], h2[n2], h3[n3];

    for (int i = 0; i < n1; i++) scanf("%d", &h1[i]);
    for (int i = 0; i < n2; i++) scanf("%d", &h2[i]);
    for (int i = 0; i < n3; i++) scanf("%d", &h3[i]);

    int result = equalStacks(h1, n1, h2, n2, h3, n3);
    printf("%d\n", result);

    return 0;
}
